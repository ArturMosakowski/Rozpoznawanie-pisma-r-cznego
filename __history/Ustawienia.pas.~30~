  Form5.CheckBox2.Checked:=False;
  Form5.CheckBox3.Checked:=False;
  Form5.CheckBox4.Checked:=True;
  Form5.CheckBox5.Checked:=False;
  Form5.CheckBox6.Checked:=False;
  Ustawienia2[0]:=4;
end;

